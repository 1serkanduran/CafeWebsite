<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/SiteLayout/Site.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>

    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="/Content/SiteLayout/images/icons/favicon.png" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="/Content/SiteLayout/vendor/bootstrap/css/bootstrap.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="/Content/SiteLayout/fonts/font-awesome-4.7.0/css/font-awesome.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="/Content/SiteLayout/fonts/themify/themify-icons.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/SiteLayout/vendor/animate/animate.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="/Content/SiteLayout/vendor/css-hamburgers/hamburgers.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="/Content/SiteLayout/vendor/animsition/css/animsition.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="/Content/SiteLayout/vendor/select2/select2.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="/Content/SiteLayout/vendor/daterangepicker/daterangepicker.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/SiteLayout/vendor/slick/slick.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="/Content/SiteLayout/vendor/lightbox2/css/lightbox.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/SiteLayout/css/util.css" />
    <link rel="stylesheet" type="text/css" href="/Content/SiteLayout/css/main.css" />
    <!--===============================================================================================-->
</head>
<body class="animsition">
    <!-- Header -->
    <!-- Header -->
    <header>
        <!-- Header desktop -->
        <div class="wrap-menu-header gradient1 trans-0-4">
            <div class="container h-full">
                <div class="wrap_header trans-0-3">
                    <!-- Logo -->
                    <div class="logo">
                        <a href="index.html">
                            <img src="/Content/SiteLayout/images/icons/logo.png"
                                 alt="IMG-LOGO"
                                 data-logofixed="/Content/SiteLayout/images/icons/logo2.png" />
                        </a>
                    </div>
                    <!-- Menu -->
                    <div class="wrap_menu p-l-45 p-l-0-xl">
                        <nav class="menu">
                            <ul class="main_menu">
                                <li>
                                    <a href="/Home/Index/">Anasayfa</a>
                                </li>
                                <li>
                                    <a href="/Home/Menu/">Menu</a>
                                </li>
                                <li>
                                    <a href="/Home/Galeri/">Galeri</a>
                                </li>
                                <li>
                                    <a href="/Home/Hakkimizda/">Hakkımızda</a>
                                </li>
                                <li>
                                    <a href="/Home/Blog/">Blog</a>
                                </li>
                                <li>
                                    <a href="/Home/Iletisim/">Bize Ulasın</a>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <!-- Social -->
                    <div class="social flex-w flex-l-m p-r-20">
                        <a href="#">
                            <i class="fa fa-tripadvisor" aria-hidden="true"></i>
                        </a>
                        <a href="#">
                            <i class="fa fa-facebook m-l-21" aria-hidden="true"></i>
                        </a>
                        <a href="#">
                            <i class="fa fa-twitter m-l-21" aria-hidden="true"></i>
                        </a>
                        <button class="btn-show-sidebar m-l-33 trans-0-4"></button>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Sidebar -->
    <aside class="sidebar trans-0-4">
        <!-- Button Hide sidebar -->
        <button class="btn-hide-sidebar ti-close color0-hov trans-0-4"></button>
        <!-- - -->
        <ul class="menu-sidebar p-t-95 p-b-70">
            <li class="t-center m-b-13">
                <a href="/Home/Index/" class="txt19">Anasayfa</a>
            </li>
            <li class="t-center m-b-13">
                <a href="/Home/Menu/" class="txt19">Menu</a>
            </li>
            <li class="t-center m-b-13">
                <a href="/Home/Galeri/" class="txt19">Galeri</a>
            </li>
            <li class="t-center m-b-13">
                <a href="/Home/Hakkimizda/" class="txt19">Hakkımızda</a>
            </li>
            <li class="t-center m-b-13">
                <a href="/Home/Blog/" class="txt19">Blog</a>
            </li>
            <li class="t-center m-b-33">
                <a href="/Home/Iletisim/" class="txt19">Bize Ulasın</a>
            </li>
        </ul>
        <!-- - -->
        <div class="gallery-sidebar t-center p-l-60 p-r-60 p-b-40">
            <!-- - -->
            <h4 class="txt20 m-b-33">Gallery</h4>
            <!-- Gallery -->
            <div class="wrap-gallery-sidebar flex-w">
                <a class="item-gallery-sidebar wrap-pic-w"
                   href="/Content/SiteLayout/images/photo-gallery-01.jpg"
                   data-lightbox="gallery-footer">
                    <img src="/Content/SiteLayout/images/photo-gallery-thumb-01.jpg" alt="GALLERY" />
                </a>
                <a class="item-gallery-sidebar wrap-pic-w"
                   href="/Content/SiteLayout/images/photo-gallery-02.jpg"
                   data-lightbox="gallery-footer">
                    <img src="/Content/SiteLayout/images/photo-gallery-thumb-02.jpg" alt="GALLERY" />
                </a>
                <a class="item-gallery-sidebar wrap-pic-w"
                   href="/Content/SiteLayout/images/photo-gallery-03.jpg"
                   data-lightbox="gallery-footer">
                    <img src="/Content/SiteLayout/images/photo-gallery-thumb-03.jpg" alt="GALLERY" />
                </a>
                <a class="item-gallery-sidebar wrap-pic-w"
                   href="/Content/SiteLayout/images/photo-gallery-05.jpg"
                   data-lightbox="gallery-footer">
                    <img src="/Content/SiteLayout/images/photo-gallery-thumb-05.jpg" alt="GALLERY" />
                </a>
                <a class="item-gallery-sidebar wrap-pic-w"
                   href="/Content/SiteLayout/images/photo-gallery-06.jpg"
                   data-lightbox="gallery-footer">
                    <img src="/Content/SiteLayout/images/photo-gallery-thumb-06.jpg" alt="GALLERY" />
                </a>
                <a class="item-gallery-sidebar wrap-pic-w"
                   href="/Content/SiteLayout/images/photo-gallery-07.jpg"
                   data-lightbox="gallery-footer">
                    <img src="/Content/SiteLayout/images/photo-gallery-thumb-07.jpg" alt="GALLERY" />
                </a>
                <a class="item-gallery-sidebar wrap-pic-w"
                   href="/Content/SiteLayout/images/photo-gallery-09.jpg"
                   data-lightbox="gallery-footer">
                    <img src="/Content/SiteLayout/images/photo-gallery-thumb-09.jpg" alt="GALLERY" />
                </a>
                <a class="item-gallery-sidebar wrap-pic-w"
                   href="/Content/SiteLayout/images/photo-gallery-10.jpg"
                   data-lightbox="gallery-footer">
                    <img src="/Content/SiteLayout/images/photo-gallery-thumb-10.jpg" alt="GALLERY" />
                </a>
                <a class="item-gallery-sidebar wrap-pic-w"
                   href="/Content/SiteLayout/images/photo-gallery-11.jpg"
                   data-lightbox="gallery-footer">
                    <img src="/Content/SiteLayout/images/photo-gallery-thumb-11.jpg" alt="GALLERY" />
                </a>
            </div>
        </div>
    </aside>
    @RenderBody()
    <!-- Footer -->
    @{
        Html.RenderAction("FooterPartial", "Home");
    }
    <!-- Back to top -->
    <div class="btn-back-to-top bg0-hov" id="myBtn">
        <span class="symbol-btn-back-to-top">
            <i class="fa fa-angle-double-up" aria-hidden="true"></i>
        </span>
    </div>
    <!-- Container Selection1 -->
    <div id="dropDownSelect1"></div>
    <!-- Modal Video 01-->
    <div class="modal fade"
         id="modal-video-01"
         tabindex="-1"
         role="dialog"
         aria-hidden="true">
        <div class="modal-dialog" role="document" data-dismiss="modal">
            <div class="close-mo-video-01 trans-0-4"
                 data-dismiss="modal"
                 aria-label="Close">
                &times;
            </div>
            <div class="wrap-video-mo-01">
                <div class="w-full wrap-pic-w op-0-0">
                    <img src="/Content/SiteLayout/images/icons/video-16-9.jpg" alt="IMG" />
                </div>
                <div class="video-mo-01">
                    <iframe src="https://www.youtube.com/embed/5k1hSu2gdKE?rel=0&amp;showinfo=0"
                            allowfullscreen></iframe>
                </div>
            </div>
        </div>
    </div>
    <!--===============================================================================================-->
    <script type="text/javascript"
            src="/Content/SiteLayout/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript"
            src="/Content/SiteLayout/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="vendor/bootstrap/js/popper.js"></script>
    <script type="text/javascript"
            src="/Content/SiteLayout/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="/Content/SiteLayout/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript"
            src="/Content/SiteLayout/vendor/daterangepicker/moment.min.js"></script>
    <script type="text/javascript"
            src="/Content/SiteLayout/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript" src="/Content/SiteLayout/vendor/slick/slick.min.js"></script>
    <script type="text/javascript" src="/Content/SiteLayout/js/slick-custom.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript"
            src="/Content/SiteLayout/vendor/parallax100/parallax100.js"></script>
    <script type="text/javascript">
        $(".parallax100").parallax100();
    </script>
    <!--===============================================================================================-->
    <script type="text/javascript"
            src="/Content/SiteLayout/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script type="text/javascript"
            src="/Content/SiteLayout/vendor/lightbox2/js/lightbox.min.js"></script>
    <!--===============================================================================================-->
    <script src="/Content/SiteLayout/js/main.js"></script>
</body>
</html>