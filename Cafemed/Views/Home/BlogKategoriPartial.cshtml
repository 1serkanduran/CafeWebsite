@model IEnumerable<Cafemed.Models.Model.Kategoriler>
<ul>
    @foreach (var item in Model)
    {
        <li class="bo5-b p-t-8 p-b-8">
            <a href="/Home/KategoriBlog/@item.KategoriId" class="txt27">
                @item.KategoriAd
            </a>
            <span>(@item.Blogs.Count)</span>
        </li>
    }

</ul>
